#include "ElementRobot.h"

ElementRobot::ElementRobot() : IElementMobile()
{

}

ElementRobot::ElementRobot(std::string name_, std::unique_ptr<se3> pose_) : IElementMobile(name_, std::move(pose_) )
{

}

ElementRobot::ElementRobot(std::string name_, std::unique_ptr <se3> pose_, const Mat<float>& hwd_) : IElementMobile(name_,std::move(pose_),hwd_)
{

}

ElementRobot::ElementRobot(std::string name_, se3* pose_, const Mat<float>& hwd_)  : IElementMobile(name_,pose_,hwd_)
{

}

ElementRobot::~ElementRobot()
{

}

bool ElementRobot::isFixe()
{
  return false;
}

bool ElementRobot::isComposit()
{
 	return false;
}

bool ElementRobot::isRobot()
{
 	return true;
}

